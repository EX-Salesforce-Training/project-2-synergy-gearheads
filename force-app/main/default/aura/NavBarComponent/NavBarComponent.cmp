<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <aura:attribute name="pageTitle" type="String" />
    <aura:attribute name="objectType" type="String" />
    <aura:attribute name="pageNumber" type="Integer" />
    <aura:attribute name="maxPage" type="Integer" />
    <div class="linkbar">
        <div>
	   <span class="nav-bar-link-wrapper">
				<a href="javascript:void(0)" onclick="{!c.ActivateHomePage}" class = 'nav-bar-link'>Home</a>
            </span>
            <span class="nav-bar-link-wrapper">
				<a href="javascript:void(0)" onclick="{!c.ActivateGuildHall}" class = 'nav-bar-link'>Guild Halls</a>
